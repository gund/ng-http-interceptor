{
  "name": "ng2-http-interceptor",
  "version": "0.0.0-semantically-relesed",
  "scripts": {
    "build": "tsc",
    "check-coverage": "istanbul check-coverage --functions 75 --lines 75 --branches 75 --statements 75",
    "ci": "npm run lint && npm run test:single && npm run check-coverage",
    "commit": "git-cz",
    "lint": "tslint src/**/*.ts",
    "prepublish": "npm run build",
    "test": "ng test",
    "test:single": "npm run test -- --watch=false",
    "test:report": "cd coverage && codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gund/ng2-http-interceptor"
  },
  "author": {
    "name": "Alex Malkevich",
    "email": "malkevich.alex@gmail.com"
  },
  "keywords": [
    "angular",
    "angular2",
    "http",
    "interceptor",
    "library"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gund/ng2-http-interceptor/issues"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/router": "3.0.1",
    "@types/jasmine": "^2.2.30",
    "angular-cli": "1.0.0-beta.16",
    "codecov": "^1.0.1",
    "codelyzer": "~0.0.26",
    "commitizen": "^2.8.6",
    "core-js": "^2.4.1",
    "cz-conventional-changelog": "^1.2.0",
    "ghooks": "^1.3.2",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.7.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "rxjs": "5.0.0-beta.12",
    "semantic-release": "^4.3.5",
    "ts-helpers": "^1.1.1",
    "ts-node": "1.4.0",
    "tslint": "3.15.1",
    "typescript": "2.0.3",
    "typings": "^1.0.4",
    "zone.js": "^0.6.23"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "echo \"Verifying commit...\" && npm run ci"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
